= form_for @rss do |f|
  - if @rss.errors.any?
    #error_explanation
      %h2= "#{pluralize(@rss.errors.count, "error")} prohibited this rss from being saved:"
      %ul
        - @rss.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
